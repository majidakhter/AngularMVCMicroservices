{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "monthly-view": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "wf",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "./config/custom-webpack.config.js"
            },
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              {
                "input": "node_modules/@angular/material/prebuilt-themes/indigo-pink.css"
              },
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/primeng/resources/themes/nova-light/theme.css",
              "node_modules/primeicons/primeicons.css",
              "node_modules/fullcalendar/dist/fullcalendar.min.css",
              "node_modules/primeflex/primeflex.css",
              "node_modules/@wfm/web-theme/css/theme.min.css",
              "src/styles.scss"
            ],
            "scripts": [
              "node_modules/moment/min/moment.min.js",
              "node_modules/fullcalendar/dist/fullcalendar.min.js",
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/tooltip.js/dist/umd/tooltip.min.js",
              "node_modules/signalr/jquery.signalR.min.js"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "baseHref": "",
              "deployUrl": ""
            },
            "sim": {
              "sourceMap": true,
              "aot": true
            },
            "int03": {
              "sourceMap": true,
              "aot": true
            },
            "int07": {
              "sourceMap": true,
              "aot": true
            },
            "int06": {
              "sourceMap": true,
              "aot": true
            },
            "int11": {
              "sourceMap": true,
              "aot": true
            },
            "local": {
              "sourceMap": true,
              "aot": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "monthly-view:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "monthly-view:build:production"
            },
            "sim": {
              "browserTarget": "monthly-view:build:sim"
            },
            "int03": {
              "browserTarget": "monthly-view:build:int03"
            },
            "int06": {
              "browserTarget": "monthly-view:build:int06"
            },
            "int07": {
              "browserTarget": "monthly-view:build:int07"
            },
            "int11": {
              "browserTarget": "monthly-view:build:int11"
            },
            "local": {
              "browserTarget": "monthly-view:build:local"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "monthly-view:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "codeCoverage": true,
            "sourceMap": true,
            "styles": [
              {
                "input": "node_modules/@angular/material/prebuilt-themes/indigo-pink.css"
              },
              {
                "input": "src/styles.scss"
              }
            ],
            "scripts": ["node_modules/jquery/dist/jquery.min.js",
              "node_modules/signalr/jquery.signalR.min.js"],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "monthly-view-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "monthly-view:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "monthly-view:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
