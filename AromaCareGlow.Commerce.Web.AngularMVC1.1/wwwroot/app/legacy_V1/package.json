{
  "name": "monthly-view",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "compodoc": "npx compodoc -p src/tsconfig.app.json -s -o",
    "coverage": "ng test --project=monthly-view --code-coverage",
    "coverage:MonthlyView": "ng test --project=MonthlyViewApp --code-coverage",
    "coverage:SelfSchedule": "ng test --project=SelfScheduleApp --code-coverage",
    "test": "ng test --project=monthly-view",
    "test:mv": "npm run test:MonthlyView",
    "test:MonthlyView": "ng test --project=MonthlyViewApp",
    "test:SelfSchedule": "ng test --project=SelfScheduleApp",
    "test-ci": "ng test --watch=false --project=monthly-view --code-coverage",
    "test-ci:MonthlyView": "ng test --watch=false --project=MonthlyViewApp --code-coverage",
    "test-ci:SelfSchedule": "ng test --watch=false --project=SelfScheduleApp --code-coverage",
    "lint": "ng lint --project=monthly-view",
    "lint:fix": "ng lint --fix=true",
    "lint:fix:MonthlyView": "ng lint --fix=true --project=MonthlyViewApp",
    "lint:fix:SelfSchedule": "ng lint --fix=true --project=SelfScheduleApp",
    "lint:MonthlyView": "ng lint --project=MonthlyViewApp",
    "lint:SelfSchedule": "ng lint --project=SelfScheduleApp",
    "lint-ci": "ng lint --project=monthly-view --format checkstyle > lint-parent.xml",
    "lint-ci:MonthlyView": "ng lint --project=MonthlyViewApp --format checkstyle > lint-monthlyview.xml",
    "lint-ci:SelfSchedule": "ng lint --project=SelfScheduleApp --format checkstyle > lint-selfschedule.xml",
    "e2e": "ng e2e --project=monthly-view",
    "e2e:MonthlyView": "ng e2e --project=MonthlyViewApp",
    "e2e:SelfSchedule": "ng e2e --project=SelfScheduleApp",
    "start:mv": "npm run start:MonthlyView",
    "start:MonthlyView": "ng serve -o --project=MonthlyViewApp",
    "start:SelfSchedule": "ng serve -o --project=SelfScheduleApp",
    "build:mv": "npm run build:MonthlyView",
    "build:MonthlyView": "npm run build:sim:MonthlyView",
    "build:SelfSchedule": "npm run build:sim:SelfSchedule",
    "build:sim:MonthlyView": "cross-env NODE_ENV=sim ng build --project=MonthlyViewApp --configuration=sim",
    "build:sim:SelfSchedule": "cross-env NODE_ENV=sim ng build --project=SelfScheduleApp --configuration=sim",
    "build:local:MonthlyView": "cross-env NODE_ENV=local ng build --project=MonthlyViewApp --configuration=local",
    "build:local:SelfSchedule": "cross-env NODE_ENV=local ng build --project=SelfScheduleApp --configuration=local",
    "build:int02:SelfSchedule": "cross-env NODE_ENV=int02 ng build --project=SelfScheduleApp --configuration=int02",
    "build:int03:MonthlyView": "cross-env NODE_ENV=int03 ng build --project=MonthlyViewApp --configuration=int03",
    "build:int03:SelfSchedule": "cross-env NODE_ENV=int03 ng build --project=SelfScheduleApp --configuration=int03",
    "build:int11:MonthlyView": "cross-env NODE_ENV=int11 ng build --project=MonthlyViewApp --configuration=int11",
    "build:int11:SelfSchedule": "cross-env NODE_ENV=int11 ng build --project=SelfScheduleApp --configuration=int11",
    "build:int06:MonthlyView": "cross-env NODE_ENV=int06 ng build --project=MonthlyViewApp --configuration=int06",
    "build:prod:mv": "npm run build:prod:MonthlyView",
    "build:prod:MonthlyView": "cross-env NODE_ENV=prod ng build --project=MonthlyViewApp --optimization=true --configuration=production --output-hashing none --base-href=\".\"",
    "build:prod:SelfSchedule": "cross-env NODE_ENV=prod ng build --project=SelfScheduleApp --optimization=true --configuration=production --output-hashing none --base-href=\".\"",
    "sim:mv": "npm run sim:MonthlyView",
    "sim:MonthlyView": "npm run build:sim:MonthlyView && ng serve -o --project=MonthlyViewApp --configuration=sim",
    "sim:SelfSchedule": "npm run build:sim:SelfSchedule && ng serve -o --project=SelfScheduleApp --configuration=sim",
    "local:mv": "npm run local:MonthlyView",
    "local:MonthlyView": "npm run build:local:MonthlyView && ng serve -o --project=MonthlyViewApp --configuration=local",
    "local:SelfSchedule": "npm run build:local:SelfSchedule && ng serve -o --project=SelfScheduleApp --configuration=local",
    "int02:SelfSchedule": "npm run build:int02:SelfSchedule && ng serve -o --project=SelfScheduleApp --configuration=int02 --proxy-config proxy.conf.json",
    "int03:mv": "npm run int03:MonthlyView",
    "int03:MonthlyView": "npm run build:int03:MonthlyView && ng serve -o --project=MonthlyViewApp --configuration=int03 --proxy-config proxy.conf.json",
    "int03:SelfSchedule": "npm run build:int03:SelfSchedule && ng serve -o --project=SelfScheduleApp --configuration=int03 --proxy-config proxy.conf.json",
    "int11:mv": "npm run int11:MonthlyView",
    "int11:MonthlyView": "npm run build:int11:MonthlyView && ng serve -o --project=MonthlyViewApp --configuration=int11 --proxy-config proxy.conf.json",
    "int11:SelfSchedule": "npm run build:int11:SelfSchedule && ng serve -o --project=SelfScheduleApp --configuration=int11 --proxy-config proxy.conf.json",
    "int06:mv": "npm run int06:MonthlyView",
    "int06:MonthlyView": "npm run build:int06:MonthlyView && ng serve -o --project=MonthlyViewApp --configuration=int06 --proxy-config proxy.conf.json",
    "prod:mv": "npm run prod:MonthlyView",
    "prod:MonthlyView": "cross-env NODE_ENV=prod ng serve -o --project=MonthlyViewApp --configuration=production",
    "prod:SelfSchedule": "cross-env NODE_ENV=prod ng serve -o --project=SelfScheduleApp --configuration=production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.1.2",
    "@angular/cdk": "^7.1.1",
    "@angular/common": "7.1.2",
    "@angular/compiler": "7.1.2",
    "@angular/core": "7.1.2",
    "@angular/forms": "7.1.2",
    "@angular/http": "7.1.2",
    "@angular/material": "^7.1.1",
    "@angular/platform-browser": "7.1.2",
    "@angular/platform-browser-dynamic": "7.1.2",
    "@angular/router": "7.1.2",
    "@api-wfm/icons": "0.0.6",
    "@api-wfm/ng-sympl-ux": "1.0.24",
    "@api-wfm/web-theme": "0.0.10",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "@ngxs/logger-plugin": "3.5.0",
    "@ngxs/storage-plugin": "3.4.3",
    "@ngxs/store": "3.4.3",
    "@types/jquery": "3.3.35",
    "@wfm/modal": "0.2.14",
    "@wfm/tooltip": "0.4.11",
    "@wfm/web-theme": "0.3.19",
    "ally.js": "1.4.1",
    "angular-calendar": "0.27.3",
    "angulartics2": "4.6.3",
    "classlist.js": "^1.1.20150312",
    "core-js": "2.5.4",
    "cross-env": "5.2.0",
    "fullcalendar": "^4.0.0-alpha.2",
    "global-tunnel-ng": "2.7.1",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "ngx-auto-unsubscribe": "2.0.0",
    "ngx-logger": "3.3.6",
    "node-sass": "4.10.0",
    "primeflex": "^1.0.0-rc.1",
    "primeicons": "^1.0.0",
    "primeng": "^8.0.2",
    "rxjs": "6.3.3",
    "rxjs-compat": "6.3.3",
    "signalr": "2.4.0",
    "tooltip.js": "^1.3.1",
    "tslib": "^1.9.0",
    "underscore": "1.9.1",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "7.0.0",
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "7.1.2",
    "@angular/compiler-cli": "7.1.2",
    "@angular/language-service": "7.1.2",
    "@api-wfm/lint": "0.0.3",
    "@compodoc/compodoc": "1.1.9",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "7.0.8",
    "@types/signalr": "2.2.35",
    "https-proxy-agent": "2.2.3",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "3.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-jquery": "0.2.4",
    "karma-junit-reporter": "^1.2.0",
    "karma-mocha-reporter": "^2.2.5",
    "protractor": "5.4.0",
    "sync-request": "6.1.0",
    "ts-node": "7.0.0",
    "tslint": "5.18.0",
    "tslint-consistent-codestyle": "1.12.0",
    "tslint-eslint-rules": "4.1.1",
    "tslint-microsoft-contrib": "5.0.3",
    "typescript": "3.1.6",
    "webpack": "4.24.0"
  }
}
