{
  "name": "yearly-view",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "compodoc": "npx compodoc -p src/tsconfig.app.json -s -o",
    "coverage": "ng test --project=yearly-view --code-coverage",
    "coverage:YearlyView": "ng test --project=YearlyViewApp --code-coverage",
    "coverage:SelfTimeline": "ng test --project=SelfTimelineApp --code-coverage",
    "test": "ng test --project=yearly-view",
    "test:mv": "npm run test:YearlyView",
    "test:YearlyView": "ng test --project=YearlyViewApp",
    "test:SelfTimeline": "ng test --project=SelfTimelineApp",
    "test-ci": "ng test --watch=false --project=yearly-view --code-coverage",
    "test-ci:YearlyView": "ng test --watch=false --project=YearlyViewApp --code-coverage",
    "test-ci:SelfTimeline": "ng test --watch=false --project=SelfTimelineApp --code-coverage",
    "lint": "ng lint --project=yearly-view",
    "lint:fix": "ng lint --fix=true",
    "lint:fix:YearlyView": "ng lint --fix=true --project=YearlyViewApp",
    "lint:fix:SelfTimeline": "ng lint --fix=true --project=SelfTimelineApp",
    "lint:YearlyView": "ng lint --project=YearlyViewApp",
    "lint:SelfTimeline": "ng lint --project=SelfTimelineApp",
    "lint-ci": "ng lint --project=yearly-view --format checkstyle > lint-parent.xml",
    "lint-ci:YearlyView": "ng lint --project=YearlyViewApp --format checkstyle > lint-yearlyview.xml",
    "lint-ci:SelfTimeline": "ng lint --project=SelfTimelineApp --format checkstyle > lint-selftimeline.xml",
    "e2e": "ng e2e --project=yearly-view",
    "e2e:YearlyView": "ng e2e --project=YearlyViewApp",
    "e2e:SelfTimeline": "ng e2e --project=SelfScheduleApp",
    "start:mv": "npm run start:YearlyView",
    "start:YearlyView": "ng serve -o --project=YearlyViewApp",
    "start:SelfTimeline": "ng serve -o --project=SelfScheduleApp",
    "build:mv": "npm run build:YearlyView",
    "build:YearlyView": "npm run build:sim:YearlyView",
    "build:SelfTimeline": "npm run build:sim:SelfTimeline",
    "build:sim:YearlyView": "cross-env NODE_ENV=sim ng build --project=YearlyViewApp --configuration=sim",
    "build:sim:SelfTimeline": "cross-env NODE_ENV=sim ng build --project=SelfTimelineApp --configuration=sim",
    "build:local:YearlyView": "cross-env NODE_ENV=local ng build --project=YearlyViewApp --configuration=local",
    "build:local:SelfTimeline": "cross-env NODE_ENV=local ng build --project=SelfTimelineApp --configuration=local",
    "build:stage02:SelfTimeline": "cross-env NODE_ENV=stage02 ng build --project=SelfTimelineApp --configuration=stage02",
    "build:stage03:YearlyView": "cross-env NODE_ENV=stage03 ng build --project=YearlyViewApp --configuration=stage03",
    "build:stage03:SelfTimeline": "cross-env NODE_ENV=stage03 ng build --project=SelfTimelineApp --configuration=stage03",
    "build:stage11:YearlyView": "cross-env NODE_ENV=stage11 ng build --project=YearlyViewApp --configuration=stage11",
    "build:stage11:SelfTimeline": "cross-env NODE_ENV=stage11 ng build --project=SelfTimelineApp --configuration=stage11",
    "build:stage06:YearlyView": "cross-env NODE_ENV=stage06 ng build --project=YearlyViewApp --configuration=stage06",
    "build:prod:mv": "npm run build:prod:MonthlyView",
    "build:prod:YearlyView": "cross-env NODE_ENV=prod ng build --project=YearlyViewApp --optimization=true --configuration=production --output-hashing none --base-href=\".\"",
    "build:prod:SelfTimeline": "cross-env NODE_ENV=prod ng build --project=SelfTimelineApp --optimization=true --configuration=production --output-hashing none --base-href=\".\"",
    "sim:mv": "npm run sim:MonthlyView",
    "sim:YearlyView": "npm run build:sim:MonthlyView && ng serve -o --project=YearlyViewApp --configuration=sim",
    "sim:SelfTimeline": "npm run build:sim:SelfSchedule && ng serve -o --project=SelfTimelineApp --configuration=sim",
    "local:mv": "npm run local:MonthlyView",
    "local:YearlyView": "npm run build:local:MonthlyView && ng serve -o --project=YearlyViewApp --configuration=local",
    "local:SelfTimeline": "npm run build:local:SelfSchedule && ng serve -o --project=SelfTimelineApp --configuration=local",
    "stage02:SelfTimeline": "npm run build:stage02:SelfSchedule && ng serve -o --project=SelfTimelineApp --configuration=stage02 --proxy-config proxy.conf.json",
    "stage03:mv": "npm run stage03:MonthlyView",
    "stage03:YearlyView": "npm run build:stage03:MonthlyView && ng serve -o --project=YearlyViewApp --configuration=stage03 --proxy-config proxy.conf.json",
    "stage03:SelfTimeline": "npm run build:stage03:SelfSchedule && ng serve -o --project=SelfTimelineApp --configuration=stage03 --proxy-config proxy.conf.json",
    "stage11:mv": "npm run stage11:MonthlyView",
    "stage11:YearlyView": "npm run build:stage11:MonthlyView && ng serve -o --project=YearlyViewApp --configuration=stage11 --proxy-config proxy.conf.json",
    "stage11:SelfTimeline": "npm run build:stage11:SelfSchedule && ng serve -o --project=SelfTimelineApp --configuration=stage11 --proxy-config proxy.conf.json",
    "stage06:mv": "npm run stage06:MonthlyView",
    "stage06:YearlyView": "npm run build:stage06:MonthlyView && ng serve -o --project=YearlyViewApp --configuration=stage06 --proxy-config proxy.conf.json",
    "prod:mv": "npm run prod:MonthlyView",
    "prod:YearlyView": "cross-env NODE_ENV=prod ng serve -o --project=YearlyViewApp --configuration=production",
    "prod:SelfTimeline": "cross-env NODE_ENV=prod ng serve -o --project=SelfTimelineApp --configuration=production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.1.2",
    "@angular/cdk": "^7.1.1",
    "@angular/common": "7.1.2",
    "@angular/compiler": "7.1.2",
    "@angular/core": "7.1.2",
    "@angular/forms": "7.1.2",
    "@angular/http": "7.1.2",
    "@angular/material": "^7.1.1",
    "@angular/platform-browser": "7.1.2",
    "@angular/platform-browser-dynamic": "7.1.2",
    "@angular/router": "7.1.2",
    "@api-wfm/icons": "0.0.6",
    "@api-wfm/ng-sympl-ux": "1.0.24",
    "@api-wfm/web-theme": "0.0.10",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "@ngxs/logger-plugin": "3.5.0",
    "@ngxs/storage-plugin": "3.4.3",
    "@ngxs/store": "3.4.3",
    "@types/jquery": "3.3.35",
    "@wfm/modal": "0.2.14",
    "@wfm/tooltip": "0.4.11",
    "@wfm/web-theme": "0.3.19",
    "ally.js": "1.4.1",
    "angular-calendar": "0.27.3",
    "angulartics2": "4.6.3",
    "classlist.js": "^1.1.20150312",
    "core-js": "2.5.4",
    "cross-env": "5.2.0",
    "fullcalendar": "^4.0.0-alpha.2",
    "global-tunnel-ng": "2.7.1",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "ngx-auto-unsubscribe": "2.0.0",
    "ngx-logger": "3.3.6",
    "node-sass": "4.10.0",
    "primeflex": "^1.0.0-rc.1",
    "primeicons": "^1.0.0",
    "primeng": "^8.0.2",
    "rxjs": "6.3.3",
    "rxjs-compat": "6.3.3",
    "signalr": "2.4.0",
    "tooltip.js": "^1.3.1",
    "tslib": "^1.9.0",
    "underscore": "1.9.1",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "7.0.0",
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "7.1.2",
    "@angular/compiler-cli": "7.1.2",
    "@angular/language-service": "7.1.2",
    "@api-wfm/lint": "0.0.3",
    "@compodoc/compodoc": "1.1.9",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "7.0.8",
    "@types/signalr": "2.2.35",
    "https-proxy-agent": "2.2.1",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "3.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-jquery": "0.2.4",
    "karma-junit-reporter": "^1.2.0",
    "karma-mocha-reporter": "^2.2.5",
    "protractor": "5.4.0",
    "sync-request": "6.1.0",
    "ts-node": "7.0.0",
    "tslint": "5.18.0",
    "tslint-consistent-codestyle": "1.12.0",
    "tslint-eslint-rules": "4.1.1",
    "tslint-microsoft-contrib": "5.0.3",
    "typescript": "3.1.6",
    "webpack": "4.24.0"
  }
}
